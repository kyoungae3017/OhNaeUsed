<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextInSplash" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/orange_500</item>
        <item name="android:textSize">@dimen/app_name_large</item>
        <item name="android:text">@string/splash_text</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextSmall" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_small</item>
    </style>

    <style name="TextMedium" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
    </style>

    <style name="TextLarge" parent="TextAppearance.AppCompat">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_large</item>
    </style>




</resources>